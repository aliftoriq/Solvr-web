{
    "version": 2,
    "name": "solvr-web",
    "alias": "solvr-web",
        "builds": [
          {
            "src": "angular.json",
            "use": "@vercel/angular"
          }
        ],
    "routes": [
      {
        "src": "/assets/(.*)",
        "dest": "dist/solvr-web/browser/assets/$1"
      },
      {
        "src": "/public/(.*)",
        "dest": "dist/solvr-web/browser/public/$1"
      },
      {
        "src": "/(.*)",
        "dest": "dist/solvr-web/server/main.js"
      }
    ]
  }