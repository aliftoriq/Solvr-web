{
    "version": 2,
    "name": "solvr-web",
    "alias": "solvr-web",
    "builds": [
        {
            "src": "dist/solvr-web/server/main.js",
            "use": "@vercel/node",
            "config": {
                "maxLambdaSize": "50mb"
            }
        },
        {
            "src": "dist/solvr-web/browser/index.html",
            "use": "@vercel/static"
        }
    ]
  }