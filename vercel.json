{
    "version": 2,
    "builds": [
      {
        "src": "dist/solvr-web/server/main.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/assets/(.*)",
        "dest": "dist/solvr-web/browser/assets/$1"
      },
      {
        "src": "/public/(.*)",
        "dest": "dist/solvr-web/browser/public/$1"
      },
      {
        "src": "/(.*)",
        "dest": "dist/solvr-web/server/main.js"
      }
    ]
  }